{
  "name": "playlist-gen",
  "version": "0.0.1",
  "description": "Generate Spotify playlists from labeled tracks",
  "scripts": {
    "build": "prisma migrate deploy && remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --write .",
    "grammar-gen": "nearleyc labelGrammar.ne > app/lib/labelGrammar.server.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@mui/material": "^5.4.1",
    "@prisma/client": "^3.9.2",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "@remix-run/server-runtime": "^1.1.3",
    "@remix-validated-form/with-zod": "^2.0.1",
    "@tinyhttp/cookie-signature": "^1.3.0",
    "dotenv": "^16.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "prisma": "^3.9.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-toastify": "^8.1.1",
    "remix": "^1.1.3",
    "remix-validated-form": "^4.1.2",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.11.6",
    "zod-form-data": "^1.1.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/lodash": "^4.14.178",
    "@types/nearley": "^2.11.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canac/playlist-gen.git"
  },
  "author": "Caleb Cox <canac@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canac/playlist-gen/issues"
  },
  "homepage": "https://github.com/canac/playlist-gen#readme"
}
