{
  "name": "playlist-gen",
  "version": "0.0.1",
  "description": "Generate Spotify playlists from labeled tracks",
  "scripts": {
    "build": "prisma migrate deploy && remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "grammar-gen": "nearleyc labelGrammar.ne > app/lib/labelGrammar.server.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0-beta3",
    "@fortawesome/free-solid-svg-icons": "^6.0.0-beta3",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@mui/material": "^5.2.8",
    "@prisma/client": "^3.8.1",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@tinyhttp/cookie-signature": "^1.3.0",
    "dotenv": "^12.0.4",
    "lodash": "^4.17.21",
    "moo": "^0.5.1",
    "nearley": "^2.20.1",
    "prisma": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^6.2.1",
    "react-toastify": "^8.1.1",
    "remix": "^1.1.1",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@types/lodash": "^4.14.178",
    "@types/nearley": "^2.11.2",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/canac/playlist-gen.git"
  },
  "author": "Caleb Cox <canac@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/canac/playlist-gen/issues"
  },
  "homepage": "https://github.com/canac/playlist-gen#readme"
}
